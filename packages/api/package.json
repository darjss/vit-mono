{
  "name": "@vit/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./lib/schema": {
      "types": "./dist/lib/schema.d.ts",
      "default": "./dist/lib/schema.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@vit/db": "workspace:*",
    "@trpc/server": "catalog:",
    "superjson": "2.2.2",
    "drizzle-orm": "^0.38.4",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "zod": "catalog:",
    "nanoid": "^5.1.3"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "@types/node": "^20",
    "typescript": "catalog:"
  }
}
